{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "variables": {},
  "resources": [
    {
      "apiVersion": "2017-05-10",
      "name": "hubvnet",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/JoeKer1/armtest/master/vnettemplate.json"
        },
        "parameters": {
          "location": {
            "value": "East US"
          },
          "vnetName": {
            "value": "hubvnet"
          },
          "addressPrefixes": {
            "value": [
              "10.0.0.0/16"
            ]
          },
          "subnets": {
            "value": [
              {
                "name": "mgmt",
                "ipRange": "10.0.0.64/27"
              },
              {
                "name": "GatewaySubnet",
                "ipRange": "10.0.255.224/27"
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "2017-05-10",
      "name": "spoke1",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/JoeKer1/armtest/master/vnettemplate.json"
        },
        "parameters": {
          "location": {
            "value": "East US"
          },
          "vnetName": {
            "value": "spoke1"
          },
          "addressPrefixes": {
            "value": [
              "10.2.0.0/16"
            ]
          },
          "subnets": {
            "value": [
              {
                "name": "mgmt",
                "ipRange": "10.2.0.64/27"
              },
              {
                "name": "workload",
                "ipRange": "10.2.1.0/24"
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "2017-05-10",
      "name": "hubtospoke1peering",
      "type": "Microsoft.Resources/deployments",
      "dependsOn": [
        "Microsoft.Resources/deployments/hubvnet",
        "Microsoft.Resources/deployments/spoke1"
      ],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/JoeKer1/armtest/master/vnetpeering.json"
        },
        "parameters": {
          "remoteVnetName": {
            "value": "spoke1"
          },
          "localVnetName": {
            "value": "hubvnet"
          }
        }
      }
    },
    {
      "apiVersion": "2017-05-10",
      "name": "spoke1-to-hub-peer",
      "type": "Microsoft.Resources/deployments",
      "dependsOn": [
        "Microsoft.Resources/deployments/hubvnet",
        "Microsoft.Resources/deployments/spoke1"
      ],
      "subscriptionId": "[subscription().subscriptionId]",
      "resourceGroup": "[resourceGroup().name]",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/JoeKer1/armtest/master/vnetpeering.json"
        },
        "parameters": {
          "remoteVnetName": {
            "value": "hubvnet"
          },
          "localVnetName": {
            "value": "spoke1"
          },
          "peeringFromHub": {
            "value": "false"
          }
        }
      }
    }
  ],
  "outputs": {}
}